{
  "name": "@bankofai/agent-wallet",
  "version": "1.0.0beta",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./wallet": {
      "types": "./dist/wallet/index.d.ts",
      "import": "./dist/wallet/index.js",
      "require": "./dist/wallet/index.js",
      "default": "./dist/wallet/index.js"
    },
    "./keystore": {
      "types": "./dist/keystore/index.d.ts",
      "import": "./dist/keystore/index.js",
      "require": "./dist/keystore/index.js",
      "default": "./dist/keystore/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "bin": {
    "agent-wallet-keystore": "dist/cli/keystore_cli.js"
  },
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\"",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "prepack": "npm run build",
    "test": "jest",
    "keystore": "node -r ts-node/register src/cli/keystore_cli.ts",
    "example:sign-message": "node -r ts-node/register examples/sign_message.ts",
    "example:local-demo": "node -r ts-node/register examples/local_demo.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "dotenv": "^17.2.4",
    "pino": "^9.9.0",
    "tronweb": "^6.2.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.2",
    "jest": "^30.2.0",
    "jest-fetch-mock": "^3.0.3",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}